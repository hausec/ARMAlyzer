{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
      "virtualMachines_WindowsAssessment_name": {
          "defaultValue": "WindowsAssessment",
          "type": "String"
      },
      "disks_WindowsAssessment_OsDisk_1_db8cfc0704a9426c83c6d2c97616db55_externalid": {
          "defaultValue": "/subscriptions/7cd95fb1-1234-4a0d-91b4-d1999a961ba4/resourceGroups/TESTING/providers/Microsoft.Compute/disks/WindowsAssessment_OsDisk_1_db8cfc0704a9426c83c6d2c97616db55",
          "type": "String"
      },
      "networkInterfaces_windowsassessment821_externalid": {
          "defaultValue": "/subscriptions/7cd95fb1-1234-4a0d-91b4-d1999a961ba4/resourceGroups/Testing/providers/Microsoft.Network/networkInterfaces/windowsassessment821",
          "type": "String"
